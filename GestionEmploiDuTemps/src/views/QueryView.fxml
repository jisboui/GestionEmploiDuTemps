<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" spacing="10" alignment="CENTER">
    <Label text="Requêtes des Emplois du Temps" />
    <GridPane hgap="10" vgap="10">
        <Label text="Classe :" />
        <ComboBox fx:id="classeComboBox" />
        <Label text="Matière :" />
        <TextField fx:id="matiereField" />
        <Button text="Afficher Séances" onAction="#handleAfficherSeances" />
    </GridPane>

    <Button text="Afficher Emploi du Temps" onAction="#handleAfficherEmploiDuTemps" />
</VBox>
